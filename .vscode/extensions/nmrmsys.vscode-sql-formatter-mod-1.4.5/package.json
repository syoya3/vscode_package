{
	"name": "vscode-sql-formatter-mod",
	"displayName": "SQL Formatter Mod",
	"description": "Format sql files using the sql-formatter-plus npm package",
	"version": "1.4.5",
	"publisher": "nmrmsys",
	"repository": {
		"type": "git",
		"url": "https://github.com/nmrmsys/vscode-sql-formatter-mod.git"
	},
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Formatters"
	],
	"keywords": [
		"sql",
		"formatter",
		"n1ql",
		"db2",
		"pl/sql"
	],
	"activationEvents": [
		"onCommand:extension.formatSQL",
		"onLanguage:sql"
	],
	"contributes": {
		"commands": [
			{
				"command": "extension.formatSQL",
				"title": "Format SQL"
			}
		],
		"configuration": {
			"type": "object",
			"title": "SQL Formatter",
			"properties": {
				"sql-formatter.dialect": {
					"scope": "resource",
					"type": "string",
					"enum": [
						"sql",
						"n1q1",
						"db2",
						"pl/sql"
					],
					"default": "sql",
					"description": "Which dialect to format with (sql: Standard SQL, n1ql: Couchbase N1QL, db2: IBM DB2, pl/sql: Oracle PL/SQL)"
				},
				"sql-formatter.uppercase": {
					"scope": "resource",
					"type": "boolean",
					"default": false,
					"description": "Convert keywords to uppercase"
				},
				"sql-formatter.linesBetweenQueries": {
					"scope": "resource",
					"type": "number",
					"minimum": 1,
					"default": 2,
					"description": "Change number of linebreaks between queries"
				}
			}
		}
	},
	"main": "./src/extension.js",
	"scripts": {
		"lint": "eslint .",
		"format": "prettier --write \"**/*.{js,jsx,md,json,css,prettierrc,eslintrc}\""
	},
	"devDependencies": {
		"eslint": "^6.6.0",
		"eslint-config-adpyke-es6": "^1.4.13",
		"eslint-config-prettier": "^6.5.0",
		"glob": "^7.1.5",
		"prettier": "^1.18.2"
	},
	"dependencies": {
		"sql-formatter-plus": "^1.3.6"
	},
	"__metadata": {
		"id": "81fcfd91-cead-42ff-8278-bbbf322ad59f",
		"publisherId": "04a5aa2c-d6ea-492c-a1d4-d8d4dc2d5c54",
		"publisherDisplayName": "nmrmsys",
		"installedTimestamp": 1619143077480
	}
}